{
  "presets": ["env", "react"],
  "sourceMaps": true,
  "ast": false,
  "minified": false,
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/plugin-transform-async-to-generator", {
      "module": "bluebird",
      "method": "coroutine"
    }]
  ],
  "ignore": ["**/*.svg"]
}
